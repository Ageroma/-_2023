#include <iostream>
#include <cstring>
#include <locale>
using namespace std;

float squared_sum(float a, float b) {
    float sum = a + b;
    return sum * sum;
}
int main() {
        setlocale(LC_ALL,"Russian");
        float a,b;
        cout << "введи число  a потом b : ";
        cin>>a;
        cin>>b;
    cout << squared_sum(a, b) << endl;
    return 0;
}
