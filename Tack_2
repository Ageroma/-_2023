#include <iostream>

using namespace std;
int main()
{
        int a; // Целые
        int s; // Целые
        float q; //Десятичные дроби
        float w; //Десятичные дроби
        char e = 'p'; // символ
        double z; // Десятичные дроби удвоенной точности
        a = 6;
        s = 3;
        q = 7.147464777777423537777777;
        w = 7.147493464777777423537777777;
        z = 1.3242342424;
        cout << a << '\n';
        cout << s << '\n';
        cout << q << '\n';
        cout << w << '\n';
        cout << e << '\n';
         cout << z << '\n';
         int A = a * s;
         cout << A << '\n'; // Умножаем целые
         float Q = q + w ;'\n';
        cout << Q << '\n' ; // Слаживаем числа с плавающей точкой
        float W = q - w ;'\n';
        cout << W << '\n' ; // Вычитаем числа с плавающей точкой
        int S = a / s;
         cout << S << '\n'; // Делим целые

        return 0 ;
}
